    "name": "Hacker",
    "tokenColors": [
        {
            "settings": {
                "foreground": "#bbbbbb"
            }
        },
        {
            "name": "Text",
            "scope": "variable.parameter.function",
            "settings": {
                "foreground": "#507057"
            }
        },
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#858585"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation.definition.string",
                "punctuation.definition.variable",
                "punctuation.definition.string",
                "punctuation.definition.parameters",
                "punctuation.definition.string",
                "punctuation.definition.array"
            ],
            "settings": {
                "foreground": "#e8e8e8"
            }
        },
        {
            "name": "Delimiters",
            "scope": "none",
            "settings": {
                "foreground": "#507057"
            }
        },
        {
            "name": "Operators",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#507057"
            }
        },
        {
            "name": "Keywords",
            "scope": "keyword",
            "settings": {
                "foreground": "#7574a5"
            }
        },
        {
            "name": "Variables",
            "scope": "variable",
            "settings": {
                "foreground": "#87a1c5"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "meta.require",
                "support.function.any-method"
            ],
            "settings": {
                "foreground": "#a7ecb7"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "support.class",
                "entity.name.class",
                "entity.name.type.class"
            ],
            "settings": {
                "foreground": "#a7ecb7"
            }
        },
        {
            "name": "Classes",
            "scope": "meta.class",
            "settings": {
                "foreground": "#858585"
            }
        },
        {
            "name": "Methods",
            "scope": "keyword.other.special-method",
            "settings": {
                "foreground": "#507057"
            }
        },
        {
            "name": "Storage",
            "scope": "storage",
            "settings": {
                "foreground": "#7574a5"
            }
        },
        {
            "name": "Support",
            "scope": "support.function",
            "settings": {
                "foreground": "#81b38c"
            }
        },
        {
            "name": "Strings, Inherited Class",
            "scope": [
                "string",
                "constant.other.symbol",
                "entity.other.inherited-class"
